    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Doc: Build - Initialize",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/cogsworth/cli/cogsw_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "document",
                "build",
                "initialize",
                "--id=c9d253c4-7b27-4507-9f0f-884bfca5d6c6",
                "--start='2023-07-14T13:44:03.112707'",
                "--branch=main",
                "--revision=fbe8ca7dcbca22c9dff506b9e9881e19cf1e3576",
                "--release='Product Core 1.0.0'",
                "${workspaceFolder}/source/examples/gen-build.json"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Doc: Build - Add By-Product",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/packages/cogsworth/cli/cogsw_command.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "document",
                "build",
                "add-by-product",
                "--product='OS Build'",
                "--platform=Device-Linux",
                "--arch=Arm",
                "--buildno=test-branch-123456",
